            <h1>Listagem usuarios</h1>
            <% if(list.length === 0){ %>
                <h2>NÃ£o existem Visitas cadastradas</h2>
            
             <% } else { %>
                <div class="">
                    <table class="table table-striped ">
                      <thead>
                      <tr>
                        <th scope="col">#</th>
                        <th scope="col">Visitante</th>
                      </tr>
                      </thead>
                      <tbody>
                    <% for (var i = 0; i < list.length; i++) { %>
